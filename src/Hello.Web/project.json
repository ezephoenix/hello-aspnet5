{
  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "dependencies": {
    "Kestrel": "1.0.0-beta6",
    "Microsoft.AspNet.Diagnostics": "1.0.0-beta6",
    "Microsoft.AspNet.Mvc": "6.0.0-beta6",
    "Microsoft.AspNet.Server.IIS": "1.0.0-beta6",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta6",
    "Microsoft.Framework.Logging.Console": "1.0.0-beta6",
    "Swashbuckle": "6.0.0-beta6",
    "Microsoft.AspNet.Cors": "1.0.0-beta6"
  },

  "commands": {
    "web": "Microsoft.AspNet.Hosting --config hosting.ini",
    "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:80"
  },

  "frameworks": {
    "dnx451": { },
    "dnxcore50": {}
  },

  "packExclude": [
    "**.kproj",
    "**.user",
    "**.vspscc"
  ],
  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}
